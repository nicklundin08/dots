{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "closer": "p:os",
    "os": "#ACB0BE",
    "pink": "#F5BDE6",
    "lavender": "#B7BDF8",
    "blue": "#8AADF4",
    "white": "#FFFFFF",
    "text": "#494D64"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "p:os",
          "background": "p:white",
          "style": "accordion",
          "template": " {{.Icon}} ",
          "type": "os"
        },
        {
          "background": "p:blue",
          "foreground": "p:text",
          "style": "accordion",
          "template": " {{ .UserName }}@{{ .HostName }} ",
          "type": "session"
        },
        {
          "background": "p:pink",
          "foreground": "p:text",
          "properties": {
            "folder_icon": "..\ue5fe..",
            "home_icon": "~",
            "style": "folder"
          },
          "style": "accordion",
          "trailing_diamond": "",
          "leading_diamond": "",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "p:lavender",
          "foreground": "p:text",
          "properties": {
            "branch_icon": "\ue725 ",
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "fetch_status": false,
            "fetch_upstream_icon": false,
            "merge_icon": "\ue727 ",
            "no_commits_icon": "\uf0c3 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 "
          },
          "template": " {{ .HEAD }} ",
          "style": "accordion",
          "type": "git"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "transient_prompt": {
    "background": "transparent",
    "foreground": "#FEF5ED",
    "template": "{{ .Shell }}"
  },
  "version": 3
}
